  <control-speed control="FLAP0" transition-time="45"/>
  <control-speed control="FLAP1" transition-time="1.0"/>
  <control-speed control="SLAT" transition-time="10.0"/>
  <control-speed control="SPOILER" transition-time="10.0"/>
