<?xml version="1.0"?>

<!--
-->

<PropertyList>

 <name>Digital Fuel Tot display</name>
 <w-base>100</w-base>
 <h-base>64</h-base>

 <layers>

  <layer>
   <name>background</name>
   <texture>
    <path>Aircraft/AN-225/Panels/Instruments/Textures/trans-dgrey-bg.rgb</path>
   </texture>
    <w>100</w>
    <h>64</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-0</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>0.8</red>
    <green>0.8</green>
    <blue>0.8</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>TOT FUEL: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-46</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>20</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>GalTitle</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>0.8</red>
    <green>0.8</green>
    <blue>0.8</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>GAL: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-46</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>5</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>GalDisplay</name>
   <type>text</type>
   <font>led</font>
   <point-size>5</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property>/consumables/fuel/total-fuel-gals</property>
     <format>%06.0f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>4</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>LBSTitle</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>0.8</red>
    <green>0.8</green>
    <blue>0.8</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>LBS: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-46</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-10</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>LBSDisplay</name>
   <type>text</type>
   <font>led</font>
   <point-size>5</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property>/consumables/fuel/total-fuel-lbs</property>
     <format>%06.0f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-11</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>PropTitle</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>0.8</red>
    <green>0.8</green>
    <blue>0.8</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>PROP: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-46</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-25</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>PropDisplay</name>
   <type>text</type>
   <font>led</font>
   <point-size>5</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property>/consumables/fuel/total-fuel-norm</property>
     <format>%01.5f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-26</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>
