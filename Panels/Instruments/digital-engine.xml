   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>N2: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>196</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>3</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>N2 display</name>
   <type>text</type>
   <font>led</font>
   <point-size>5</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property alias="../../../../../params/n2-select"/>
     <format>%03.f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>223</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>3</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>FF-Title</name>
   <type>text</type>
   <point-size>14</point-size>
   <color>
    <red>0.5</red>
    <green>0.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>FF: </text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>258</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>3</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
