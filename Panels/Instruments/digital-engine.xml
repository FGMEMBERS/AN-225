    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>3</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Th display</name>
   <type>text</type>
   <font>led</font>
   <point-size>5</point-size>
   <color>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property alias="../../../../../params/th-select"/>
     <format>%01.3f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>355</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>3</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>
